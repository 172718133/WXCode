page{
  background-color: #edece8;
}
.btn{
  height: 45vh;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 44rpx;
  color: rgb(138, 206, 218);
}
.tologin{
  padding: 20rpx;
  border: 1px solid rgb(138, 206, 218);
  border-radius: 10rpx;
}


.user_bg{
  height: 45vh;
  filter: blur(10rpx);
}
.user_img{
  position: relative;
}
.user_infos{
  height: 45vh;
  overflow: hidden;
}
.user_info{
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 20%;
  text-align: center;
}
.user_head{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}
.user_name{
  color: #ffffff;
  font-size: 42rpx;
  margin-top: 40rpx;
}
.user_content{
  position: relative;
}
.user_main{
  padding-bottom: 100rpx;
  color: #666;
  position: absolute;
  width: 90%;
  left: 50%;
  transform: translateX(-50%);
  top: -40rpx;
}
.history_info{
  display: flex;
  background-color: #ffffff;
}
.history_info navigator {
  flex: 1;
  text-align: center;
  padding: 20rpx;
}
.his_num{
  color: var(--themeColor);
  margin-bottom: 8rpx;
}
.order_info{
  margin-top: 30rpx;
  background-color: #fff;
}
.order_my{
  padding: 20rpx;
  border-bottom: 1rpx solid #ccc;
}
.order_about{
  display: flex;
}
.iconfont{
  color: var(--themeColor);
  font-size: 50rpx;
  margin-bottom: 10rpx;
}
.order_about navigator{
  padding: 15rpx;
  flex: 1;
  text-align: center;
}
.address,.share{
  font-size: 30rpx;
  padding: 30rpx;
  margin-top: 30rpx;
  background-color: #fff;
}
.app_info{
  font-size: 30rpx;
  margin-top: 30rpx;
  background-color: #fff;
}
.app_item{
  padding: 30rpx;
  border-bottom: 1rpx solid #ccc;
}
.contact{
  display: flex;
  justify-content: space-between;
}