.index_swiper swiper{
  width: 750rpx;
  height: 340rpx;
}
.index_swiper image{
  width: 100%;
}

.index_cate{
  display: flex;
}
.index_cate navigator{
  padding: 20rpx;
  flex: 1;
}
.index_cate image{
  width: 100%;
}


/* 楼层css样式 */

.index_float{
  padding: 0 10rpx;
}
.index_title{
  padding: 10rpx 0;
}
.index_title image{
  width: 100%;
}
.index_list{
  overflow: hidden;
  margin-bottom: 10rpx;
}
.index_list navigator{
  float: left;
  width: 33.33%;
}
.index_list navigator:nth-last-child(-n+4){
  /* height: 33.33vw * 386 / 232 / 2  ; */
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_list navigator:nth-child(2),.index_list navigator:nth-child(3){
  border-bottom: 10rpx solid #fff;
}
.index_list navigator image{
  width: 100%;
  height: 100%;
}